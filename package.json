{
  "name": "playwright-xray-testimporter",
  "main": "lib/index.js",
  "types": "lib/testimporter.d.ts",
  "version": "0.0.4",
  "description": "Utility to extract test steps from Playwright and import them into Xray",
  "private": false,
  "scripts": {
    "prepare": "npm run build",
    "version": "npm run format",
    "build": "tsc && npm run add-js",
    "release": "release-it --github.release",
    "release:ci": "npm run release -- --ci --npm.skipChecks --no-git.requireCleanWorkingDir",
    "release:patch": "npm run release -- patch",
    "release:minor": "npm run release -- minor",
    "release:major": "npm run release -- major",
    "add-js": "ts-add-js-extension --dir=lib",
    "format": "npx biome format --write",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NiklasBack/playwright-xray-testimporter.git"
  },
  "author": "Niklas Back <niklas.back@gmail.com>",
  "license": "MIT",
  "bin": {
    "xraytestimporter": "./lib/index.js"
  },
  "bugs": {
    "url": "https://github.com/NiklasBack/playwright-xray-testimporter/issues"
  },
  "homepage": "https://github.com/NiklasBack/playwright-xray-testimporter#readme",
  "devDependencies": {
    "@biomejs/biome": "2.2.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.30",
    "jest": "^29.7.0",
    "msw": "^2.10.2",
    "release-it": "^19.0.4",
    "ts-add-js-extension": "^1.6.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.3"
  },
  "directories": {
    "lib": "lib"
  },
  "dependencies": {
    "axios": "^1.7.7"
  }
}
